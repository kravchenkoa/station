profile = "vcs/user-repos@2.0"
  provider = "bitbucket"
  
  map UserRepos{
    http GET "/2.0/repositories?role=member" {
    security "basic"

    response 200 "application/json" {
      map result {
        //map returned values to result object
        repos = body.values.map((r) => {
          return {
            name: r.name,
            description: r.description
          }
        })
      }
    }
  }
}
